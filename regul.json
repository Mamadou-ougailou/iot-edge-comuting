    {
        "id": "6e42aa47ddb0dea2",
        "type": "function",
        "z": "8639d94574434ac4",
        "name": "Format Serial Data",
        "func": "let lt = flow.get('lt') || 26.0;\nlet ht = flow.get('ht') || 25.8;\nif (msg.topic === 'lt') {\n    lt = msg.payload;\n    flow.set('lt', lt);\n} else if (msg.topic === 'ht') {\n    ht = msg.payload;\n    flow.set('ht', ht);\n}\nmsg.payload = `LT:${lt.toFixed(1)},HT:${ht.toFixed(1)}\\n`;\nreturn msg;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "\nlet lt = 26.0;\nlet ht = 25.8;",
        "finalize": "",
